<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.simulation.dao.GreenWaveMapper">
    <insert id="insertList" parameterType="java.util.List">
        INSERT INTO simulation_green_wave(traffic_light_id,next_id,interval_second,group_id)VALUES
        <foreach collection="list" separator="," item="item">
            (#{item.trafficLightId},#{item.nextId},#{item.intervalSecond},#{item.groupId})
        </foreach>
    </insert>
</mapper>